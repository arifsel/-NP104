def add(x, y):
  print (x+y)
def sub(x, y):
  print (x-y)
def mul(x, y):
  print (x*y)
def Div(x, y):
  print (x/y)

x=int(input("zahl1="))
y=int(input("zahl2="))
operator=input("operation: ")
if operator=="+":
  add(x,y)
elif operator=="-":
  sub(x,y)
elif operator=="*":
  mul(x,y)
elif operator=="/":
  div(x,y)
else:
  print("Fehler")
f=input("neue aufgabe (Y/N)")
if f=="Y"or f=="y":
  continue
elif f=="N"or f=="n":
  break
print("Ende")
  
